					0
					0
				]}
			change \{structurename = player1_status
				save_num_powerups = 0}
			change \{structurename = player2_status
				save_num_powerups = 0}
			change \{structurename = player1_status
				save_health = 0.0}
			change \{structurename = player2_status
				save_health = 0.0}
		endif
	endif
