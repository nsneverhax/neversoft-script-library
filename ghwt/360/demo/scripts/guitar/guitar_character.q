	if compositeobjectexists name = <name>
		<name> :hero_play_strum_anim anim = ($<strum_anims>.no_strum_anim)
		<name> :hero_play_fret_anim anim = ($<fret_anims>.track_96)
		<name> :hero_play_finger_anim anim = ($<finger_anims>.track_none)
