script crowd_anticipation 
	if ($current_crowd >= 1.3399999)
		if issoundplaying \{$current_crowd_anticipation}
			if ($crowd_anticipation_flag = 0)
				stopsound \{$current_crowd_anticipation
					fade_time = 3
					fade_type = log_slow}
