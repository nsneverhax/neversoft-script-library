			{pad_choose net_custom_ui_change_unfocus params = {action = choose to = <vmenu_id> from = <song_vmenu_id> menu = song_type}}
		]
		exclusive_device = ($primary_controller)
	}
	CreateScreenElement {
		Type = TextElement
		id = <song_text_id>
