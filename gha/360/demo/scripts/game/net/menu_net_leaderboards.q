		parent = <cont_id>
		tex = window_frame_cap
		rgba = ($online_medium_blue)
		pos = <top_pos>
		just = [left top]
		z_priority = <base_z>
	}
	scale_element_to_size {
		id = <id>
		target_width = (<top_dims>.(1.0, 0.0))
		target_height = (<top_dims>.(0.0, 1.0))
	}
	displaysprite {
		parent = <cont_id>
		tex = window_fill_cap
		rgba = <bg_fill_color>
		pos = <top_pos>
		just = [left top]
		z_priority = <base_z>
	}
	scale_element_to_size {
		id = <id>
		target_width = (<top_dims>.(1.0, 0.0))
		target_height = (<top_dims>.(0.0, 1.0))
	}
	getscreenelementdims id = <id>
	middle_pos = (<top_pos> + <height> * (0.0, 1.0))
