script songlist_get_mode 
	songlist_component :membfunc_songlist_uistack_getmode
	return mode = <mode>
endscript

script songlist_change_sort_index \{index = !i1768515945}
	Change g_songlist_sort_index = <index>
endscript

script songlist_change_sort_by_key \{key = !q1768515945}
	GetArraySize \{$g_songlist_sort_array}
	<i> = 0
	begin
	if ((($g_songlist_sort_array) [<i>].key) = <key>)
		Change g_songlist_sort_index = <i>
		return
	endif
	<i> = (<i> + 1)
	repeat <array_Size>
	ScriptAssert \{qs(0x25cfff5e)}
endscript

script songlist_set_mode_sort_index 
	RequireParams \{[
			mode
		]
		all}
	switch (<mode>)
		case play_song
		Change structurename = g_songlist_sort_indexes play_song = ($g_songlist_sort_index)
		case playlist
		Change structurename = g_songlist_sort_indexes playlist = ($g_songlist_sort_index)
		case music_store
		Change structurename = g_songlist_sort_indexes music_store = ($g_songlist_sort_index)
		case leaderboard
		Change structurename = g_songlist_sort_indexes leaderboard = ($g_songlist_sort_index)
		case practice
		Change structurename = g_songlist_sort_indexes practice = ($g_songlist_sort_index)
	endswitch
endscript

script songlist_get_header_array \{curr_header_type = !q1768515945}
	<header_type> = (($g_songlist_sort_array [($g_songlist_sort_index)]).header_type)
	switch <header_type>
		case genre
		case Source
		return header_type = <header_type>
		default
		return header_array = (($g_songlist_headers).<header_type>) header_type = <header_type>
	endswitch
endscript

script songlist_set_header_array \{header_type = !q1768515945
		header_array = !a1768515945}
	updatestructelement {
		struct = $g_songlist_headers
		element = <header_type>
		value = <header_array>
	}
	Change g_songlist_headers = <newstruct>
endscript

script songlist_get_sort_key 
	<sort_key> = (($g_songlist_sort_array [($g_songlist_sort_index)]).key)
	return sort_key = <sort_key>
endscript

script songlist_change_filterspec 
	RequireParams \{[
			filterspec
		]
		all}
	if ScreenElementExists \{id = songlist_component}
		if GotParam \{invert_filterspec}
			songlist_component :membfunc_songlist_uistack_setfilterspec filterspec = <filterspec> invert_filterspec = 1
		else
			songlist_component :membfunc_songlist_uistack_setfilterspec filterspec = <filterspec>
		endif
	endif
endscript

script songlist_clear_filterspec 
	if ScreenElementExists \{id = songlist_component}
		songlist_component :membfunc_songlist_uistack_setfilterspec \{filterspec = [
			]}
	endif
endscript

script songlist_force_num_songs_to_choose 
	RequireParams \{[
			num_songs
		]
		all}
	Change g_songlist_force_num_songs_to_choose = <num_songs>
endscript

script songlist_dont_force_num_songs 
	Change \{g_songlist_force_num_songs_to_choose = 0}
endscript

script is_a_playlist_loaded 
	GetArraySize \{$current_playlist}
	if (<array_Size> > 0)
		return true size = <array_Size>
	else
		return \{FALSE}
	endif
endscript

script is_current_playlist_empty 
	if is_a_playlist_loaded
		return \{FALSE}
	else
		return \{true}
	endif
endscript

script allow_songlist_options_writes 
	gamemode_gettype
	if (<Type> = freeplay)
		return \{FALSE}
	endif
	band_lobby_get_is_guest device_num = ($g_gbm_savegame_device)
	if ((islocallysignedin controller = $g_gbm_savegame_device) && (<is_guest> = 0))
		get_savegame_from_controller controller = ($g_gbm_savegame_device)
		if get_savegame_has_saved_game savegame = <savegame>
			if is_autosave_on savegame = <savegame>
				return \{true}
			endif
		endif
	endif
	return \{FALSE}
endscript
