script GetMovementArchetype 
	agent_gettype
	Agent_HasEnemy
	switch <type>
		case combat
		switch <agent_type>
			case cow
			case horse
			case mounted_horse
			case rider
			<movement_desc> = horse_urgent_object_move
			case ped
			case hostage
			<movement_desc> = ped_urgent_object_move
			default
			printstruct <...>
			scriptassert "unknown movement archetype"
		endswitch
		case ObjectArrival
		if gotparam enemy
			switch <agent_type>
				case cow
				case horse
				case mounted_horse
				case rider
				<movement_desc> = horse_urgent_object_move
				case ped
				case hostage
				<movement_desc> = ped_urgent_object_move
				default
				printstruct <...>
				scriptassert "unknown movement archetype"
			endswitch
		else
			switch <agent_type>
				case cow
				case horse
				case mounted_horse
				case rider
				<movement_desc> = horse_lazy_object_move
				case ped
				case hostage
				<movement_desc> = ped_lazy_object_move
				default
				printstruct <...>
				scriptassert "unknown movement archetype"
			endswitch
		endif
		case PointArrival
		if gotparam enemy
			switch <agent_type>
				case cow
				case horse
				case mounted_horse
				case rider
				<movement_desc> = horse_urgent_point_move
				case ped
				case hostage
				<movement_desc> = ped_urgent_point_move
				default
				printstruct <...>
				scriptassert "unknown movement archetype"
			endswitch
		else
			switch <agent_type>
				case cow
				case horse
				case mounted_horse
				case rider
				<movement_desc> = horse_lazy_point_move
				case ped
				case hostage
				<movement_desc> = ped_lazy_point_move
				default
				printstruct <...>
				scriptassert "unknown movement archetype"
			endswitch
		endif
		default
		printstruct <...>
		scriptassert "unknown movement archetype"
	endswitch
	return {movement_desc = <movement_desc>}
endscript
