script build_cas_editable_list 
	if (<cas_type> = CAS_DECKS)
		return cas_editable_list = ($master_editable_list_first + $master_editable_list_last)
	endif
	if (<cas_type> = CAS_PROS)
		return cas_editable_list = ($master_editable_list_pro)
	endif
	if (<cas_type> = CAS_CAREER)
		return cas_editable_list = ($master_editable_list_career)
	endif
	scriptassert \{'Eeek'}
endscript

script build_master_editable_list 
	cas_editable_list = []
	return master_editable_list = ($master_editable_list_first + $master_editable_list_career + $master_editable_list_pro + <cas_editable_list> + $master_editable_list_last)
endscript
