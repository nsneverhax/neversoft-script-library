GlobalCollideMuteDelayTime = 0

script bouncy_sfx_maincallback 
	if NOT iscreated <objid1>
		return
	endif
	getstarttime
	if ($GlobalCollideMuteDelayTime > <starttime>)
		return
	endif
	CollideMuteDelayTime = 0
	<objid1> :getsingletag CollideMuteDelayTime
	if (<CollideMuteDelayTime> > <starttime>)
		return
	endif
	sound_type = none
	<objid1> :rigidbody_getsoundtype
	if NOT structurecontains structure = $rigidbodysounds <sound_type>
		return
	endif
	addparams ($rigidbodysounds.<sound_type>)
	if NOT gotparam bouncevelocitycallbackthreshold
		return
	endif
	if (<bouncevelocitycallbackthreshold> < <projectedvelocity>)
		if (<skatercollision> = true)
			soundevent event = <SkaterHitSoundEvent> velocity = <projectedvelocity>
		else
			soundevent event = <BounceSoundEvent> velocity = <projectedvelocity>
			Bouncy_SFX_Bounce {terrain = (($terrain_types [<terrain2>]).checksum)
				velocity = <projectedvelocity>
				object = <objid1>}
		endif
		change GlobalCollideMuteDelayTime = (<starttime> + <globalcollidemutedelay>)
		<objid1> :settags CollideMuteDelayTime = (<starttime> + <collidemutedelay>)
	endif
endscript

script Bouncy_SFX_SkaterHit 
endscript

script Bouncy_SFX_Bounce 
	switch <terrain>
		case terrain_dirt
		case terrain_grass
		case terrain_grassdried
		case terrain_sand
		case terrain_leaves
		soundevent event = GrassBounce_SFX <...>
		case terrain_wood
		case terrain_wooddeck
		case terrain_wood_4ft
		case terrain_wood_8ft
		case terrain_woodfence
		case terrain_woodsiding
		case terrain_woodtable
		case terrain_woodplywood
		case terrain_shinglescomposite
		soundevent event = WoodBounce_SFX <...>
		case terrain_plastichard
		case terrain_plasticpoolcover
		case terrain_plasticpicnictable
		soundevent event = PlasticBounce_SFX <...>
		case terrain_metalcar
		case terrain_metal
		case terrain_metalpole
		case terrain_metaltin
		case terrain_metallockers
		case terrain_metalthin
		case terrain_metalpipelarge
		case terrain_mailbox
		case terrain_metalgrate
		soundevent event = MetalBounce_SFX <...>
		case terrain_watershallow
		case terrain_waterpuddle
		case terrain_water
		soundevent event = WaterBounce_SFX <...>
		default
		if (debugsoundfx = 1)
		endif
	endswitch
endscript
GrassBounce_SFX_struct = {
	default_priority = 10
	default_buss = Bouncy1
	norepeatfor = 0.1
	instancemanagement = stop_furthest
	instancelimit = 5
}

script GrassBounce_SFX 
	bounce_volume = (<velocity> * 4)
	bounce_pitch = (100 + (<velocity> / 2))
	if (<bounce_volume> > 200)
		bounce_volume = 200
	endif
	RandomNoRepeat (
		@ obj_playsound sk8_terr_grass_run_thick_01 vol = <bounce_volume> pitch = <bounce_pitch> dropoff = 15
		@ obj_playsound sk8_terr_grass_run_thick_02 vol = <bounce_volume> pitch = <bounce_pitch> dropoff = 15
		@ obj_playsound sk8_terr_grass_run_thick_03 vol = <bounce_volume> pitch = <bounce_pitch> dropoff = 15
		)
endscript
