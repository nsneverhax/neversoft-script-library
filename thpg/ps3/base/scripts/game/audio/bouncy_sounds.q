	bounce_pitch = (110 + (<velocity> / 2))
	if (<bounce_volume> > 200)
		bounce_volume = 200
	endif
	RandomNoRepeat (
		@ obj_playsound SK8_Bouncy_WoodTable_Bounce_01 vol = <bounce_volume> pitch = <bounce_pitch> dropoff = 15
		@ obj_playsound SK8_Bouncy_WoodTable_Bounce_02 vol = <bounce_volume> pitch = <bounce_pitch> dropoff = 15
		@ obj_playsound SK8_Bouncy_WoodTable_Bounce_03 vol = <bounce_volume> pitch = <bounce_pitch> dropoff = 15
		)
endscript
