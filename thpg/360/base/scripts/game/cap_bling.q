script CAP_Fill_Bling_Object_Structs 
	getarraysize \{$CAP_Bling_Object_Types}
	<i> = 0
	begin
	CAP_Fill_Bling_Object_Struct type = ($CAP_Bling_Object_Types [<i>])
	<i> = (<i> + 1)
	repeat <array_size>
endscript

script CAP_Fill_Bling_Object_Struct type = bling
	getarraysize $cap_bling_object_raw_list
	new_array = []
	i = 0
	begin
	src = (($cap_bling_object_raw_list) [<i>])
	if ((<src>.type) = <type>)
		formattext textname = path 'Props/%s/%t' s = (<src>.model) t = (<src>.model)
		formattext checksumname = csum '%s' s = (<src>.model)
		if structurecontains structure = <src> collision
			formattext textname = collision 'Props/%s_coll/%t_coll' s = (<src>.model) t = (<src>.model)
		else
			collision = <path>
		endif
		if ((<src>.name) = "Mattress")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Bed")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Canopy Bed")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Futuristic Sofa")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Fourpost Bed")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Leather Sofa")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Tiger Rug")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Recliner")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Thrift Eofa")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Race Car Bed 01")
			Sounds2use = CAP_Sliding_Mattress
		elseif ((<src>.name) = "Jeep Patriot")
			Sounds2use = CAP_Sliding_Car
		elseif ((<src>.name) = "Vintage Car")
			Sounds2use = CAP_Sliding_Car
		elseif ((<src>.name) = "Stretch Limo")
			Sounds2use = CAP_Sliding_Car
		elseif ((<src>.name) = "Bulldozer")
			Sounds2use = CAP_Sliding_Car
		elseif ((<src>.name) = "Jeep Liberty")
			Sounds2use = CAP_Sliding_Car
		elseif ((<src>.name) = "Imitation Tiger Rug")
			Sounds2use = CAP_Sliding_Mattress
		else
			Sounds2use = CAP_Sliding_Sounds_Light_Metal
		endif
		new_elem = {
			name = <csum>
			name_string = (<src>.name)
			model = <path>
			collision = <collision>
			cost = (<src>.cost)
			sounds = <Sounds2use>
			type = (<src>.type)
			limit = (<src>.limit)
			is_valid_script = cap_is_valid_crib_piece_available
			raw_index = <i>
			$CAP_Bling_Object_Settings
		}
		if structurecontains structure = <src> norail
			new_elem = (<new_elem> + {norail})
		endif
		if structurecontains structure = <src> state
			new_state = (<src>.state)
			new_elem = (<new_elem> + {state = <new_state>})
		endif
		if structurecontains structure = <src> fxType
			new_FXType = (<src>.fxType)
			new_elem = (<new_elem> + {fxType = <new_FXType>})
		endif
		if structurecontains structure = <src> prox_activate
			new_additions = {
				components = [{component = objectproximity}]
				create_script = crib_obj_prox_created
				create_script_params = {type = (<src>.prox_activate) index = <i>}
			}
			new_elem = (<new_elem> + <new_additions>)
		endif
		addarrayelement array = <new_array> element = <new_elem>
		new_array = <array>
	endif
	i = (<i> + 1)
	repeat <array_size>
	<temp_structure> = {($CAP_Bling_Objects)}
	addparam structure_name = temp_structure name = <type> value = <new_array>
	change CAP_Bling_Objects = {<temp_structure>}
endscript

script CAP_Count_Bling_Objects 
	getarraysize <array>
	bling_count = 0
	index_count = 0
	i = 0
	if (<array_size> > 0)
		begin
		type = ((<array> [<i>]).piece_type)
		piece_idx = ((<array> [<i>]).piece_index)
		if (<type> = bling)
			bling_count = (<bling_count> + 1)
			if gotparam index
				if (<piece_idx> = <index>)
					index_count = (<index_count> + 1)
				endif
			endif
		endif
		i = (<i> + 1)
		repeat <array_size>
	endif
	if gotparam index
		return count = <index_count>
	else
		return count = <bling_count>
	endif
endscript

script CAP_Get_Bling_Object_Limit 
	obj = ($cap_bling_object_raw_list [<index>])
	return limit = (<obj>.limit)
endscript
